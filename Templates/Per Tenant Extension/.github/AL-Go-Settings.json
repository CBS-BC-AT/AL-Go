{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go-Actions/main/settings.schema.json",
  "buildModes": [
    "Default"
  ],
  "customCodeCops": [
    "https://github.com/StefanMaron/BusinessCentral.LinterCop/releases/latest/download/BusinessCentral.LinterCop.dll"
  ],
  "doNotPublishApps": true,
  "enableCodeCop": true,
  "enablePerTenantExtensionCop": true,
  "enableUICop": true,
  "githubRunner": "ubuntu-latest",
  "rulesetFile": "CegekaAT.ruleset.json",
  "runs-on": "ubuntu-latest",
  "skipUpgrade": true,
  "BcContainerHelperVersion": "latest",
  "templateUrl": "https://github.com/CBS-BC-AT/AL-Go@main",
  "templateFiles": [
    ".github/DeployTo*.ps1",
    ".schemas/*"
  ],
  "type": "PTE",
  "useCompilerFolder": true,
  "vsixFile": "latest"
}
